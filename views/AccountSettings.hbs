	top: 0;
	left: 0;
	box-sizing: border-box;
	transform: scale(0);
	transition: .5s;
}
.outline::before {
	border-bottom: 3px solid #d48d00;
	border-left: 3px solid #d48d00;
	transform-origin: 0 100%;
}
.outline::after {
	border-top: 3px solid #d48d00;
	border-right: 3px solid #d48d00;
	transform-origin: 100% 0%;
}
.usermenu-link-gold-wrap:hover::before, .usermenu-link-gold-wrap:hover::after {
	transform: scale(1);
}
.usermenu-link-gold {
	width: 100%;
	color: #d48d00;
	margin: auto;
	text-align: left;
	text-decoration: none;
